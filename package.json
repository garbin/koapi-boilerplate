{
  "name": "koapi-boilerplate",
  "version": "0.1.0",
  "description": "RESTful API framework based on koajs",
  "main": "build/app.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "start": "export NODE_ENV=production && node --harmony server.js",
    "test": "export NODE_ENV=test && npm run db:rollback && npm run db:setup && make test",
    "build": " export NODE_ENV=production && rm -rf ./build ./storage/public/* && webpack --progress --colors && babel -d build/ src/",
    "dev:server": "NODE_ENV=development nodemon -L -e js,es --harmony server.js",
    "dev:ui": "webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --host 0.0.0.0",
    "test:coverage": "export NODE_ENV=test && npm run db:rollback && npm run db:setup && make test-cover"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garbin/koapi-boilerplate.git"
  },
  "keywords": [
    "RESTful",
    "API",
    "Framework",
    "koa"
  ],
  "author": "Garbin Huang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garbin/koapi-boilerplate/issues"
  },
  "homepage": "https://github.com/garbin/koapi-boilerplate#readme",
  "dependencies": {
